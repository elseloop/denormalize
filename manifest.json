{
"manifest_version": 2,
    "name": "Denormalize",
    "description": "Replaces the phrase ‘alt-right’ with ‘neo-Nazi,’ because words matter.",
    "version": "1.0",
	"default_locale": "en",
	"author": "elseloop",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
